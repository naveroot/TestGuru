<div class="card border-primary">
  <h3 class="card-title px-3 py-3 text-center"><%= t('feedback.header') %></h3>
  <div class="card-body">
    <%= form_with model: feedback, url: feedback_index_path(feedback), local: true  do |form| %>
      <div class="form-group">
        <p>
          <%= form.label t('feedback.title') %>
          <%= form.text_field :title, class: 'form-control' %>
        </p>
        <p>
          <%= form.label t('feedback.body') %>
          <%= form.text_field :body, class: 'form-control' %>
        </p>
        <%= form.submit class: 'btn btn-primary'%>
        <%= link_to t('links.back'), tests_path, class: 'btn'%>
    <% end %>
    </div>
  </div>
</div>
